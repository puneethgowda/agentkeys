{
  "name": "agentkeys",
  "version": "0.1.0",
  "description": "Self-hosted API key manager for AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/puneethgowda/agentkeys.git",
    "directory": "packages/server"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "agentkeys": "./dist/index.js"
  },
  "files": ["dist", "dashboard"],
  "scripts": {
    "dev": "tsx watch src/index.ts -- serve",
    "build": "tsup src/index.ts --format esm --dts --clean && cp -r ../dashboard/dist dashboard 2>/dev/null || true",
    "start": "node dist/index.js serve",
    "test": "vitest run"
  },
  "keywords": ["ai", "api-keys", "agents", "secret-manager", "vault", "self-hosted"],
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.13.0",
    "argon2": "^0.41.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "drizzle-orm": "^0.38.0",
    "hono": "^4.6.0",
    "jose": "^5.9.0",
    "nanoid": "^5.0.9",
    "ora": "^8.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
